									<xs:documentation>Кадастровый инженер</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="Cadastral_Engineer" type="tCadastral_Engineer">
											<xs:annotation>
												<xs:documentation>Физическое лицо</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Cadastral_Organization" type="tCadastral_Organization">
											<xs:annotation>
												<xs:documentation>Юридическое лицо</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:choice>
									<xs:attribute name="Date" type="xs:date" use="required">
										<xs:annotation>
											<xs:documentation>Дата завершения кадастровых работ</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element ref="Coord_Systems" minOccurs="0"/>
							<xs:element name="Client" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сведения о заказчике кадастровых работ</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="Person">
											<xs:annotation>
												<xs:documentation>Физические лица</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="FIO" type="tFIO">
														<xs:annotation>
															<xs:documentation>Фамилия, имя, отчество для Физ. лиц</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Organization">
											<xs:annotation>
												<xs:documentation>Юридические лица</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Name">
														<xs:annotation>
															<xs:documentation>Полное наименование</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Agent">
														<xs:annotation>
															<xs:documentation>Представитель</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="tFIO">
																	<xs:sequence>
																		<xs:element name="Appointment" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Должность</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:maxLength value="255"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																	</xs:sequence>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Governance">
											<xs:annotation>
												<xs:documentation>Орган государственной власти, орган местного самоуправления</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Name">
														<xs:annotation>
															<xs:documentation>Полное наименование</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Agent">
														<xs:annotation>
															<xs:documentation>Представитель</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="tFIO">
																	<xs:sequence>
																		<xs:element name="Appointment" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Должность</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:maxLength value="255"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																	</xs:sequence>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Foreign_Organization">
											<xs:annotation>
												<xs:documentation>Иностранное юридическое лицо</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Name">
														<xs:annotation>
															<xs:documentation>Полное наименование</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Country">
														<xs:annotation>
															<xs:documentation>Наименование страны регистрации (инкорпорации)</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="255"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Agent">
														<xs:annotation>
															<xs:documentation>Представитель</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="tFIO">
																	<xs:sequence>
																		<xs:element name="Appointment" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Должность</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:maxLength value="255"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																	</xs:sequence>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:choice>
									<xs:attribute name="Date" type="xs:date" use="required">
										<xs:annotation>
											<xs:documentation>Дата приемки работ</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Input_Data" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Исходные данные</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Documents" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Перечень документов, использованных при подготовке технического плана</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Document" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Документ</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="tDocument">
																	<xs:sequence>
																		<xs:element name="Kind" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Наименование (вид) картографического произведения</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:maxLength value="255"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																		<xs:element name="Scale" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Масштаб картографического произведения</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:maxLength value="255"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																		<xs:element name="Form" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Форма картографического произведения</xs:documentation>
																			</xs:annotation>
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:maxLength value="255"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																		<xs:element name="Date_Create" type="xs:date" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Дата создания картографического произведения</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="Date_Update" type="xs:date" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Дата последнего обновления картографического произведения</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																	</xs:sequence>
																</xs:extension>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Geodesic_Bases" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о геодезической основе, использованной при подготовке технического плана</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Geodesic_Base" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Пункт геодезической сети</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="PName">
																	<xs:annotation>
																		<xs:documentation>Название пункта геодезической сети</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:maxLength value="4000"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="PKind">
																	<xs:annotation>
																		<xs:documentation>Тип пункта геодезической сети</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:maxLength value="4000"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="PKlass">
																	<xs:annotation>
																		<xs:documentation>Класс геодезической сети</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:maxLength value="4000"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="OrdX">
																	<xs:annotation>
																		<xs:documentation>Координата Х</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:decimal">
																			<xs:totalDigits value="38"/>
																			<xs:fractionDigits value="2"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="OrdY">
																	<xs:annotation>
																		<xs:documentation>Координата У</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:decimal">
																			<xs:totalDigits value="38"/>
																			<xs:fractionDigits value="2"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Means_Survey" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о средствах измерения</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Means_Survey" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Сведения о средствах измерения</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Name">
																	<xs:annotation>
																		<xs:documentation>Название прибора (инструмента, аппаратуры)</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:maxLength value="1500"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="Certificate">
																	<xs:annotation>
																		<xs:documentation>Реквизиты сертификата прибора (инструмента, аппаратуры)</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:maxLength value="4000"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="Certificate_Verification">
																	<xs:annotation>
																		<xs:documentation>Реквизиты свидетельства о поверке прибора (инструмента, аппаратуры)</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:maxLength value="4000"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Survey" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сведения о выполненных измерениях и расчетах</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Geopoint_Opred" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Метод определения координат характерных точек контура здания</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Element" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Элемент</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="Number">
																<xs:annotation>
																	<xs:documentation>Номер контура</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="Num_Geopoint" use="required">
																<xs:annotation>
																	<xs:documentation>Номер точки</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:integer">
																		<xs:totalDigits value="22"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="Geopoint_Opred" use="required">
																<xs:annotation>
																	<xs:documentation>Метод определения координат</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="50"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Tochn_Geopoint" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Точность определения координат характерных точек контура здания</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Element" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Элемент</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Formula" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation>Формулы, примененные для расчета средней квадратической погрешности определения координат характерных точек контура</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:maxLength value="4000"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="Number">
																<xs:annotation>
																	<xs:documentation>Номер контура</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="Num_Geopoint" use="required">
																<xs:annotation>
																	<xs:documentation>Номер точки</xs:documentation>
