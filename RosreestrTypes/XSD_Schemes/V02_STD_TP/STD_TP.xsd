	<xs:element name="STD_TP">
		<xs:annotation>
			<xs:documentation>ОСНОВНОЙ КОРНЕВОЙ ЭЛЕМЕНТ</xs:documentation>
		</xs:annotation>
		<xs:complexType>

      <xs:sequence>
     <!-- <xs:choice> -->
				<xs:element name="Building">
					<xs:annotation>
						<xs:documentation>Технический план здания</xs:documentation>
					</xs:annotation>
					<xs:complexType>
            
						<xs:sequence>
							<xs:element name="Package">
								<xs:annotation>
									<xs:documentation>Пакет информации</xs:documentation>
								</xs:annotation>
								<xs:complexType>
                  <xs:sequence>
                    <!-- <xs:choice> -->
                  <xs:element name="New_Building" type="tNew_Building">
											<xs:annotation>
												<xs:documentation>Постановка на государственный кадастровый учет здания</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Exist_Building" type="tExist_Building">
											<xs:annotation>
												<xs:documentation>Государственный кадастровый учет изменений здания </xs:documentation>
											</xs:annotation>
										</xs:element>
                      <!--                     </xs:choice> -->
                    </xs:sequence>                                      
								</xs:complexType>
							</xs:element>
							<xs:element name="Contractor" minOccurs="0">
