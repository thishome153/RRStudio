											</xs:simpleType>
										</xs:element>
										<xs:element name="Appointment">
											<xs:annotation>
												<xs:documentation>Должность удостоверяющего</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="255"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="FIO">
											<xs:annotation>
												<xs:documentation>Инициалы, фамилия удостоверяющего</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
                

              
              </xs:sequence>
					</xs:complexType>
				</xs:element>

        <!-- До сюда Package  -->   
        
				<xs:element name="Reestr_Contractors" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Реестр исполнителей  работ</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
              <!--Заменим здесь 
							<xs:element name="Reestr_Contractor" type="tReestr_Contractor" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Реестр исполнителей  работ</xs:documentation>
