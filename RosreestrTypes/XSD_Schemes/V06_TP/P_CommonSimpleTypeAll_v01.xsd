			<xs:documentation>Признак контура (0 - Наземный контур, 1 - Подземный контур, 2 - Надземный контур)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>Наземный контур</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Подземный контур</xs:documentation>
